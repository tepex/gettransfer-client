<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EditableView">
        <attr name="hint" format="string"/>
    </declare-styleable>

    <declare-styleable name="SearchAddress" parent="EditableView">
        <attr name="hint"/>
    </declare-styleable>

    <declare-styleable name="SearchAddressReadOnly" parent="EditableView">
        <attr name="hint"/>
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="CreateOrderField" parent="EditableView">
        <attr name="hint"/>
        <attr name="icon_img" format="reference" />
        <attr name="isFocusable" format="boolean"/>
        <attr name="showChevron" format="boolean"/>
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="TransferDetailsField" parent="EditableView">
        <attr name="title_transfer_details" format="string" />
        <attr name="input_transfer_details" format="string"/>
    </declare-styleable>

    <declare-styleable name="AboutItem" parent="EditableView">
        <attr name="title_about_item" format="string" />
        <attr name="img_about_item" format="reference" />
    </declare-styleable>

    <declare-styleable name="RateField">
        <attr name="rate_title" format="string"/>
        <attr name="rate_sub_title" format="string" />
    </declare-styleable>

    <declare-styleable name="TransferMainInfoItemView">
        <attr name="transfer_main_info_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="CommunicationButton">
        <attr name="btn_img" format="reference"/>
        <attr name="btn_name" format="string"/>
        <attr name="btn_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension"/>
        <attr name="defaultCustomState" format="enum">
            <enum name="stateAnchorPoint" value="3" />
            <enum name="stateExpanded" value="4"/>
            <enum name="stateCollapsed" value="5"/>
            <enum name="stateHidden" value="6"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ContactsView">
        <attr name="icon_country" format="reference"/>
        <attr name="title_country" format="string"/>
        <attr name="phone" format="string"/>
    </declare-styleable>

    <declare-styleable name="SettingsFieldHorizontalPicker">
        <attr name="settingsTitlePicker"/>
        <attr name="settingsPickerDivider"/>
        <attr name="settingsPickerBlackText"/>
    </declare-styleable>

    <declare-styleable name="SettingsFieldVerticalPicker">
        <attr name="settingsTitlePicker"/>
    </declare-styleable>

    <attr name="settingsTitlePicker" format="string"/>
    <attr name="settingsPickerDivider" format="boolean"/>
    <attr name="settingsPickerBlackText" format="boolean"/>

    <declare-styleable name="SettingsEditableField">
        <attr name="settingsEditableFieldHint" format="string"/>
        <attr name="settingsEditableFieldEnabled" format="boolean"/>
        <attr name="settingsEditableFieldChevron" format="boolean"/>
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="SettingsFieldSwitch">
        <attr name="settingsTitleSwitch" format="string"/>
        <attr name="settingsShowDivider" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SwitcherView">
        <attr name="switcher_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="SearchFormMainView">
        <attr name="ivSelectFieldToGone" format="boolean"/>
        <attr name="ivSelectFieldFromGone" format="boolean"/>
        <attr name="roundedTopCorners" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RatingFieldView">
        <attr name="rating_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="ChildCounterView">
        <attr name="counter_title" format="string"/>
        <attr name="counter_description" format="string"/>
    </declare-styleable>

    <declare-styleable name="InputPasswordView">
        <attr name="password_view_hint" format="string"/>
        <attr name="password_view_divider_visible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="InputAccountFieldView">
        <attr name="account_field_view_hint" format="string"/>
        <attr name="account_field_view_divider_visible" format="boolean"/>
        <attr name="account_field_view_icon" format="reference"/>
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="TransferDetailsCarrierInfoField">
        <attr name="carrier_field_title" format="string"/>
        <attr name="carrier_field_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="VehiclePhotosView">
        <attr name="multiple_photos_width" format="dimension"/>
        <attr name="multiple_photos_height" format="dimension"/>
        <attr name="single_photo_height" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="NetworkNotAvailableView">
        <attr name="text_no_internet" format="string"/>
    </declare-styleable>
</resources>
